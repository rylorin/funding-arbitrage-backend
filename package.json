{
  "name": "funding-arbitrage-backend",
  "version": "1.0.0",
  "description": "Backend for crypto funding rate arbitrage platform",
  "keywords": [
    "crypto",
    "arbitrage",
    "funding",
    "defi"
  ],
  "license": "MIT",
  "author": "Funding Arbitrage Team",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "dev": "ts-node-dev --project tsconfig.json -r tsconfig-paths/register src/index",
    "disabled-postinstall": "cd ./node_modules/@x10xchange/examples/typescript && npm install",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "start": "node dist/index.js",
    "test": "jest --maxWorkers=8 --reporters=default --useStderr --detectOpenHandles",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "vest:register": "ts-node --project tsconfig.json -r tsconfig-paths/register src/utils/vest-cli.ts"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@noble/curves": "^2.0.1",
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1",
    "@x10xchange/stark-crypto-wrapper-wasm": "0.1.6",
    "axios": "^1.13.2",
    "bignumber.js": "^9.3.1",
    "bs58": "^6.0.0",
    "config": "^4.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.7.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "hyperliquid": "^1.7.7",
    "joi": "^18.0.1",
    "json-custom-numbers": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "node-cron": "^4.2.1",
    "pg": "^8.11.1",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.6",
    "starknet": "^8.7.0",
    "uuid": "^13.0.0",
    "winston": "^3.10.0",
    "ws": "^8.13.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.17",
    "@types/node-cron": "^3.0.8",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.46.3"
  },
  "engines": {
    "node": ">=20.19.2"
  }
}
